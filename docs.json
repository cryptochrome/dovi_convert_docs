{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "dovi_convert Documentation",
  "colors": {
    "primary": "#ea76cb",
    "light": "#ea76cb",
    "dark": "#f5c2e7"
  },
  "favicon": "/images/dovi_convert-icon-round-cropped.png",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "icon": "globe",
          "href": "https://mintlify.com/docs"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "the-why"
            ]
          },
          {
            "group": "Installation",
            "expanded": true,
            "pages": [
              "installation/terminal",
              "installation/docker"
            ]
          },
          {
            "group": "Core Concepts",
            "expanded": true,
            "pages": [
              "essentials/scanning",
              "essentials/conversion",
              "essentials/batch",
              "essentials/backup-restore"
            ]
          }
        ]
      },
      {
        "tab": "Command Referecnce",
        "pages": [
          "command-referecnce/coming-soon"
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/cryptochrome/dovi_convert"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/cryptochrome/dovi_convert"
    }
  },
  "description": "Everything you need to know about dovi_convert",
  "background": {
    "color": {
      "light": "#eff1f5",
      "dark": "#1e1e2e"
    }
  },
  "appearance": {
    "default": "system"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "integrations": {
    "telemetry": {
      "enabled": false
    }
  },
  "logo": "/images/dovi_convert-icon-round-cropped.png"
}
