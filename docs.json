{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "dovi_convert Documentation",
  "colors": {
    "primary": "#7287fd",
    "light": "#7287fd",
    "dark": "#b4befe"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "icon": "globe",
          "href": "https://mintlify.com/docs"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/cryptochrome/dovi_convert"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/cryptochrome/dovi_convert"
    }
  },
  "description": "Everything you need to know about dovi_convert",
  "background": {
    "color": {
      "light": "#eff1f5",
      "dark": "#1e1e2e"
    }
  },
  "appearance": {
    "default": "system"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "integrations": {
    "telemetry": {
      "enabled": false
    }
  }
}
